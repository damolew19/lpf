{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "rules": {
        "semi": ["error", "always"],
        // "quotes": ["error", "double"],
        "react/prop-types": 0
    },
    "plugins": ["react", "import", "jsx-a11y"],
     "settings": {
        "react": {
          "version": "detect"
        }
    }
}